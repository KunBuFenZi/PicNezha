{
	"version": 2,
	"builds": [{
		"src": "app.js",
		"use": "@vercel/node",
		"config": {
			"installCommand": "apt-get update && apt-get install -y libfontconfig1 && npm install"
		}
	}],
	"routes": [{
		"src": "/(.*)",
		"dest": "/app.js",
		"methods": [
			"GET",
			"POST"
		],
		"headers": {
			"x-vercel-serverless-handler": "true",
			"x-vercel-ignore": "false"
		},
		"continue": true
	}]
}
